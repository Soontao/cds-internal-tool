{
  "name": "cds-internal-tool",
  "version": "1.4.0",
  "main": "lib/index.js",
  "description": "internal tools for CAP nodejs extension",
  "author": {
    "name": "Theo Sun"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-internal-tool"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "devDependencies": {
    "@types/jest": "28.1.7",
    "@types/node": "16.11.51",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "axios": "0.27.2",
    "eslint": "8.22.0",
    "eslint-plugin-jest": "26.8.4",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "license": "Apache-2.0",
  "keywords": [
    "CAP",
    "cds",
    "utils"
  ],
  "dependencies": {
    "@types/express": "^4.17.13"
  }
}
