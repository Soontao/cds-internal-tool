{
  "name": "cds-internal-tool",
  "version": "1.7.5",
  "main": "lib/index.js",
  "description": "internal tools for CAP nodejs extension",
  "author": {
    "name": "Theo Sun"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-internal-tool"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "18.17.6",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "axios": "1.4.0",
    "eslint": "8.47.0",
    "eslint-plugin-jest": "27.2.3",
    "jest": "29.6.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "license": "Apache-2.0",
  "keywords": [
    "CAP",
    "cds",
    "utils"
  ],
  "dependencies": {
    "@types/express": "4.17.17"
  },
  "optionalDependencies": {
    "fsevents": "2.3.3"
  }
}
