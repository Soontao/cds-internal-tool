{
  "name": "cds-internal-tool",
  "version": "0.0.29",
  "main": "lib/index.js",
  "description": "internal tools for CAP nodejs extension",
  "author": {
    "name": "Theo Sun"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Soontao/cds-internal-tool"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "axios": "^0.27.2",
    "commitlint": "^16.3.0",
    "eslint": "^8.16.0",
    "eslint-plugin-jest": "^26.4.5",
    "husky": "~4.3.8",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "Apache-2.0",
  "keywords": [
    "CAP",
    "cds",
    "utils"
  ],
  "dependencies": {
    "@types/express": "^4.17.13"
  }
}
